generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ListExtinguisher {
  id                  String   @id @default(uuid())
  status              String
  name                String
  numberManufecturyng String
  dateFabri           DateTime
  dateTest            DateTime
  place               String
  area                String
}
